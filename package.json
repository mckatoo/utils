{
  "name": "@mckatoo/utils",
  "version": "0.1.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "run-s clean build:cjs build:esm copy-esm-package",
    "prepublishOnly": "run-s build",
    "copy-esm-package": "copyfiles configs/_package.esm.json dist/esm/package.json",
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Milton Carlos Katoo <mckatoo@gmail.com> (https://github.com/mckatoo)",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "winston": "^3.17.0"
  }
}
